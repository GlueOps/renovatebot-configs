{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "customManagers": [],
  "packageRules": [
    {
      "description": "Automerge non-major updates",
      "matchUpdateTypes": [
        "patch",
        "digest",
        "pin",
        "pinDigest",
        "bump",
        "minor"
      ],
      "automerge": true
    }
  ],
  "extends": [
    ":semanticCommits"
  ]
}